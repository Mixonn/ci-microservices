{
    "services": [
        {
            "name": "ElasticSearch",
            "port": 9200,
            "check": {
                "id": "ElasticSearch-health",
                "name": "HTTP health",
                "http": "http://es:9200/_cluster/health",
                "interval": "10s",
                "timeout": "1s"
            }
        },
        {
            "name": "Kibana",
            "port": 5601,
            "check": {
                "id": "Kibana-health",
                "name": "HTTP health",
                "http": "http://kibana:5601/api/status",
                "interval": "60s",
                "timeout": "5s"
            }
        }
    ]
}
